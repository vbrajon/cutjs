{
  "name": "cut",
  "version": "1.0.0",
  "description": "Shortcut utility library to speed up development.",
  "main": "cut.js",
  "type": "module",
  "bin": {
    "cute": "cute.js",
    "cutest": "cutest.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vbrajon/cut.git"
  },
  "keywords": [
    "shortcut",
    "utility"
  ],
  "author": "Valentin Brajon",
  "license": "MIT",
  "devDependencies": {
    "date-fns": "*",
    "lodash-es": "*",
    "moment": "*",
    "underscore": "*",
    "sugar": "*"
  },
  "optionalDependencies": {
    "eslint": "*",
    "eslint-config-prettier": "*",
    "eslint-config-standard": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-node": "*",
    "eslint-plugin-prettier": "*",
    "eslint-plugin-promise": "*",
    "prettier": "*"
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 160,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
