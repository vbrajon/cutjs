{
  "name": "cut",
  "type": "module",
  "bin": {
    "cute": "cute.js",
    "cutest": "cutest.js"
  },
  "scripts": {
    "lint": "npx prettier --no-semi --print-width=Infinity --write .",
    "test": "TZ=Europe/Paris deno test --import-map import-map.json --allow-read --allow-net --no-check --watch",
    "test:bench": "TZ=Europe/Paris deno bench --import-map import-map.json --allow-read --allow-net --no-check --watch --unstable cut-sync.test.js",
    "test:coverage": "TZ=Europe/Paris deno test --import-map import-map.json --allow-read --allow-net --no-check --coverage=cov;deno coverage cov;/bin/rm -rf cov"
  }
}
